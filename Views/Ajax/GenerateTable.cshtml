
<table border="1" cellpadding="5pt">
    <tr>
        <th>Number</th>
        <th>Multiplier</th>
        <th>Result</th>
    </tr>

    @for(var i = 1; i <= ViewBag.Length; i ++)
    {
    <tr>
        <td>@ViewBag.Number</td>
        <td>@i</td>
        <td>@(ViewBag.Number * i)</td>
    </tr>
    }
</table>
